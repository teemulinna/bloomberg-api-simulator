# AI Model Configuration
# @ruvector/agentic-synth will use these keys for AI-powered data generation

# Google Gemini (Primary - Recommended for agentic-synth)
# Get your API key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI (Alternative for agentic-synth)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude (Alternative for agentic-synth)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenRouter (Alternative - supports 50+ models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Azure OpenAI (Fallback for news generation)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=gpt-4  # Your deployment name
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Bloomberg API Simulation Settings
PORT=8080
WS_PORT=8081
MAX_SYMBOLS=100
DEFAULT_MARKET_MODE=normal
ENABLE_SELF_LEARNING=true
CACHE_TTL=3600
MAX_CACHE_SIZE=1000
